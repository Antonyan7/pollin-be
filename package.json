    "lodash": "^4.17.21",
    "mailgun.js": "^12.0.1",
    "mysql2": "^3.14.0",
    "nestjs-i18n": "^10.5.0",
    "pdf-merger-js": "4.3.1",
    "pdfmake": "0.2.14",
    "protobufjs": "^7.4.0",
