  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:service": "nest build $SERVICE && cp ./package.json ./dist/apps/$SERVICE/ && cp ./yarn.lock ./dist/apps/$SERVICE/ && cp .env.example ./dist/apps/$SERVICE/ && cp ./project.toml ./dist/apps/$SERVICE/ &&  cp -R ./Nestproject-eslint-plugin ./dist/apps/$SERVICE/Nestproject-eslint-plugin",
    "build:core": "SERVICE=core yarn run build:service",
    "build:core-background": "SERVICE=core-background yarn run build:service",
    "build:swagger": "SERVICE=swagger yarn run build:service",
    "build:download": "SERVICE=download yarn run build:service",
    "build:clinic-ivf-lab": "SERVICE=clinic-ivf-lab yarn run build:service",
    "build:lis": "SERVICE=lis yarn run build:service",
    "build:lis-background": "SERVICE=lis-background yarn run build:service",
    "build:emr": "SERVICE=emr yarn run build:service",
    "build:emr-background": "SERVICE=emr-background yarn run build:service",
    "build:booking": "SERVICE=booking yarn run build:service",
    "build:booking-background": "SERVICE=booking-background yarn run build:service",
    "build:all": "nest build core && nest build download && nest build lis && nest build emr && nest build booking && nest build core-background && nest build emr-background && nest build lis-background && nest build booking-background",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "node ./main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug core": "nest start --debug --watch core",
    "start:debug core-background": "nest start --debug --watch core-background",
    "start:debug clinic-ivf-lab": "nest start --debug --watch clinic-ivf-lab",
    "start:debug lis": "nest start --debug --watch lis",
    "start:debug lis-background": "nest start --debug --watch lis-background",
    "start:debug emr": "nest start --debug --watch emr",
    "start:debug emr-background": "nest start --debug --watch emr-background",
    "start:debug booking": "nest start --debug --watch booking",
    "start:debug booking-background": "nest start --debug --watch booking-background",
    "start:debug download": "nest start --debug --watch download",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,tools/src,functional-tests}/**/*.ts\" dangerfile.ts",
    "test": "jest --no-cache --config jest.json",
    "test:watch": "jest --watch",
    "test:cov": "node --no-compilation-cache --expose-gc ./node_modules/jest/bin/jest.js --config jest.json --coverage --logHeapUsage --runInBand .spec.ts",
    "test:no-cov": "node --no-compilation-cache --expose-gc ./node_modules/jest/bin/jest.js --config jest.json --logHeapUsage --runInBand .spec.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
    "test:e2e": "jest --config ./apps/core/test/jest-e2e.json",
    "test:clear-cache": "jest --clearCache",
    "audit:all": "set -e; find . -name 'package.json' -not -path '*/node_modules/*' |  while read package_path ; do echo '\n\nAudit for' $package_path && npx audit-ci@^7 --config ./audit-ci.jsonc --directory ${package_path%package.json}; done",
    "clear:dist": "rm -rf ./dist",
    "docker:create-e2e-testing-image": "docker build -t backend-e2e-testing -f ./e2e-build/Dockerfile .",
    "build:e2e-testing-image": "yarn run clear:dist && yarn run build:all && yarn run docker:create-e2e-testing-image",
    "version": "echo $npm_package_version",
    "run:tool": "IS_TOOLS_ENV=true ts-node -r tsconfig-paths/register ./tools",
    "script:pre-commit": "node ./scripts/pre-commit.js",
    "prepare": "husky || true"
  },
